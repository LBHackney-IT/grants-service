{
  "name": "discretionary-business-grants",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "sls deploy",
    "sls-debug": "export SLS_DEBUG=* && node --inspect ./node_modules/serverless/bin/serverless offline -s staging --noAuth --noPrependStageInUrl",
    "preint-test": "yarn build",
    "unit-test": "jest",
    "unit-test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && echo 'Lint complete.'",
    "format": "prettier '**/*.{js,jsx,css,scss,md}' --write --list-different",
    "dbmigrate": "db-migrate --migrations-dir 'db/migrations/'",
    "dbmigratetest": "db-migrate --migrations-dir 'db/migrations/' -e test",
    "dbmigratedry": "db-migrate --dry-run --migrations-dir 'db/migrations/'",
    "cypress-run": "cypress run --browser firefox",
    "cypress-open": "cypress open --browser firefox",
    "int-test": "start-server-and-test start http://localhost:3000 cypress-run",
    "remote:migrate": "sls invoke --function database-migrator"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "classnames": "^2.2.6",
    "cookie": "^0.4.1",
    "date-fns": "^2.14.0",
    "dateformat": "^4.0.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "file-type": "^17.1.1",
    "formidable": "^2.0.1",
    "google-spreadsheet": "^3.0.13",
    "govuk-frontend": "^4.0.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mockdate": "^3.0.2",
    "nanoid": "^3.1.5",
    "next": "^12.1.0",
    "node-lambda-authorizer": "LBHackney-IT/node-lambda-authorizer.git#0fe9e7514882c8ffb4182fa984ee10b99bfd886c",
    "notifications-node-client": "^4.7.3",
    "objects-to-csv": "^1.3.6",
    "pg": "^8.7.1",
    "pg-promise": "^10.5.6",
    "react": "17.0.2",
    "react-beforeunload": "^2.2.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^5.7.2",
    "react-table": "^7.2.0",
    "restana": "^4.3.4",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.4.1",
    "uk-postcode-validator": "^1.1.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "aws-sdk": "^2.669.0",
    "babel-jest": "^27.4.6",
    "cypress": "^9.3.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "faker": "^4.1.0",
    "husky": ">=4",
    "jest": "^27.4.7",
    "lint-staged": ">=10",
    "prettier": "^2.2.0",
    "prop-types": "^15.7.2",
    "sass": "^1.26.8",
    "serverless": "^2.72.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5"
  },
  "optionalDependencies": {
    "@babel/core": "7.x"
  },
  "resolutions": {
    "mixme": "^0.5.4",
    "ssh2": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --list-different"
  }
}
